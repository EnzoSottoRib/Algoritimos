#include <stdio.h>

int main(){
    
    int num=1, cont;
    
    while(num != 0){
        
        printf("Digite um numero: ");
        scanf("%d", &num);
        if(num == 0){
            break;
        }
        if(num >= 100 && num <= 200){
            cont += 1;
        }
    }
    
    printf("Foram digitados %d numeros entre 100 e 200!", cont);
    
    return 0;
}
